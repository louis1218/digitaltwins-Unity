  <gazebo reference="SonarBack_frame">
    <sensor name="SonarBack" type="ray">
      <pose>0 0 0 0 0 0</pose>
      <update_rate>20</update_rate>
      <visualize>false</visualize>
      <ray>
        <scan>
          <horizontal>
            <samples>5</samples>
            <resolution>3</resolution>
            <min_angle>-0.52</min_angle>
            <max_angle>0.52</max_angle>
          </horizontal>
          <vertical>
            <samples>5</samples>
            <resolution>3</resolution>
            <min_angle>-0.52</min_angle>
            <max_angle>0.52</max_angle>
          </vertical>
        </scan>
        <range>
          <origin rpy="1.57 0.0 0.0 " xyz="0.09 -0.155 -0.264"/>
          <min>0.0</min>
          <max>5.0</max>
          <resolution>3</resolution>
        </range>
      </ray>
      <plugin filename="libgazebo_ros_ray_sensor.so" name="gazebo_ros_range">
        <robotNamespace>/pepper</robotNamespace>
        <gaussianNoise>0.05</gaussianNoise>
        <alwaysOn>true</alwaysOn>
        <updateRate>20</updateRate>
        <topicName>sonar_back</topicName>
        <frameName>SonarBack_frame</frameName>
        <minRange>0.0</minRange>
        <maxRange>5.0</maxRange>
        <fov>1.05</fov>
